{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__init__.py": true,
        "**/__pycache__": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/*.pyc": true,
        "**/node_modules": true,
        ".vscode": true
    },
    "explorerExclude.backup": {},
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    }
}